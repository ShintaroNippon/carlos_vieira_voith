{
  "name": "cv-git-hub-finder",
  "version": "1.1.1",
  "private": true,
  "description": "A frontend boilerplate with React hooks, Redux and Typescript",
  "homepage": "https://github.com/ShintaroNippon/carlos_vieira_voith",
  "keywords": [
    "react",
    "hooks",
    "redux",
    "typescript",
    "boilerplate",
    "searchmvc"
  ],
  "main": "index.js",
  "scripts": {
    "lint:fix": "eslint './src/**/*.{ts,tsx}'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack -p --progress --colors"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "3.10.7",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/history": "^4.7.3",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/webpack": "^4.41.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.1.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "jest-fetch-mock": "3.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^7.0.25",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^2.1.2",
    "react-hot-loader": "^4.12.18",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.1.3",
    "ts-jest": "26.4.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.7.4",
    "url-loader": "^4.0.0",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "classnames": "^2.2.6",
    "eslint": "^6.6.0",
    "isomorphic-unfetch": "^3.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^10.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
